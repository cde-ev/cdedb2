#: cdedb/frontend/templates/web/core/show_user.tmpl:613
msgid "Restores the archives Account. It will be usable again."
msgstr ""
"Stellt den archivierten Account wieder her. Er kann anschließend wieder "
"normal genutzt werden."

#: cdedb/frontend/templates/web/core/show_user.tmpl:604
#: cdedb/frontend/templates/web/core/show_user.tmpl:605
#: cdedb/frontend/templates/web/core/show_user.tmpl:625
msgid "Delete Account"
msgstr "Account leeren"

#: cdedb/frontend/templates/web/core/show_user.tmpl:610
#: cdedb/frontend/templates/web/core/show_user.tmpl:611
#: cdedb/frontend/templates/web/core/show_user.tmpl:631
