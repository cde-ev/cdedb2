        \BLOCK{ for part_id, part in ambience['event']['parts']|xdictsort('part_begin') }
            \begin{wrapfigure}{r}{3.1cm}
                \vspace{-2\baselineskip}
                \begin{center}
                    \includegraphics[keepaspectratio=true, width=3cm,height=3cm]{./aka-logo}
                \end{center}
                \vspace{-2\baselineskip}
            \end{wrapfigure}
            \section*{\VAR{ lodgement['moniker']|te } (\VAR{ part['title']|te })}
            \begin{itemize}
                \BLOCK{ for registration_id in inhabitants[(lodgement_id, part_id)] }
                    \item
                        \VAR{ personas[registrations[registration_id]['persona_id']]['given_names']|te }
                        \VAR{ personas[registrations[registration_id]['persona_id']]['family_name']|te }
                \BLOCK{ else }
                    \item \BLOCK{ trans } niemand \BLOCK{ endtrans }
                \BLOCK{ endfor }
            \end{itemize}
            \clearpage
        \BLOCK{ endfor }
    \BLOCK{ endfor }
\BLOCK{ endblock }
