{% set sidenav_active='core_manage_archive' %}
{% extends "web/core/base.tmpl" %}
{% import "web/util.tmpl" as util with context %}
{% import "web/generic.tmpl" as generic with context %}
{% set jshint='strong' %}
{% block scripts %}
    {{ util.cdedb_script('cdedb_queryform.js') }}
    {{ util.cdedb_script('cdedb_helper.js') }}
{% endblock %}
{% block title %}
    {% trans %}
        Full User Search
    {% endtrans %}
{% endblock %}
{% block breadcrumb %}
    {{ super() }}
    {{ util.breadcrumb_link(cdedblink("core/full_user_search"), gettext("Search All Users"), active="True") }}
{% endblock %}
{% block content %}
    {{ generic.user_search(scope, result, query) }}
{% endblock %}
