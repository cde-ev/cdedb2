{% set sidenav_active='assembly_attendees' %}
{% extends "web/de/assembly/base.tmpl" %}
{% block title %}Anwesenheitsliste -- {{ assembly_data['title']|e }}{% endblock %}
{% block content %}
    <p>Insgesamt {{ user_data|length|e }} Anwesende.</p>
    <ul>
        {% for persona_id, pdata in user_data|dictsort %}
            <li>{{ pdata['given_names']|e }} {{ pdata['family_name']|e }}</li>
        {% endfor %}
    </ul>
{% endblock %}
