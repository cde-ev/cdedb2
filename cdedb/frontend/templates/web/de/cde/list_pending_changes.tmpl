{% extends "web/de/cde/base.tmpl" %}
{% import "web/de/util.tmpl" as util with context %}
{% block title %}List der zu begutachtenden Änderungen{% endblock %}
{% block content %}
   <h1>Zurzeit liegen {{ pending|length|e }} Änderungen vor</h1>
   <ul>
        {% for persona_id, entry in pending.items() %}
           <li><a href="{{ cdedblink('cde/inspect_change', {'persona_id' : persona_id})|e }}">{{ entry['given_names']|e }} {{ entry['family_name']|e }}</a></li>
        {% endfor %}
   </ul>
{% endblock %}
