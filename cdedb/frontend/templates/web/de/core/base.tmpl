{% extends "web/de/base.tmpl" %}
{% import "web/de/util.tmpl" as util with context %}
{% block core_navigation %}
    <ul>
        {% if is_admin %}
            <li>
                <form action="{{ cdedblink('core/admin_show_user')|e }}" method="GET"
                 id="adminshowuserform">
                    <div>
                        {{ util.input_text(name="id_to_show") }}
                        {{ util.input_submit(value="ID") }}
                    </div>
                </form>
            </li>
            <li>{{ util.href(cdedblink("core/genesis_list_cases"), "Accountanfragen") }}</li>
            <li>{{ util.href(cdedblink("core/view_log"), "Account-Log") }}</li>
        {% endif %}
    </ul>
{% endblock %}
