{% set sidenav_active='event_list' %}
{% extends "web/de/event/base.tmpl" %}
{% import "web/de/util.tmpl" as util with context %}
{% block title %}Alle DB-Veranstaltungen{% endblock %}
{% block breadcrumb %}
{{ util.breadcrumb_begin() }}
{{ util.breadcrumb_link(cdedblink("event/index"), "Veranstaltungen", active="True") }}
{{ util.breadcrumb_end() }}
{% endblock %}
{% block content %}
    <ul>
        {% for event_id, entry in data|dictsort|reverse %}
            <li>
                {{ util.href(cdedblink("event/show_event", {"event_id": event_id}), entry['title']) }}
            </li>
        {% endfor %}
    </ul>
{% endblock %}
